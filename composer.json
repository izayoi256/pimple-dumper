{
    "name": "izayoi256/pimple-dumper",
    "description": "Pimple Dumper - automatically generates pimple.json",
    "type": "library",
    "license": "MIT",
    "keywords" : [
        "pimple",
        "autocomplete",
        "di",
        "dependency injection",
        "container",
        "pimple.json"
    ],
    "authors": [
        {
            "name": "Shotaro Hama",
            "email": "qwert.izayoi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-json": "*",
        "pimple/pimple": "~2.0|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Izayoi256\\PimpleDumper\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izayoi256\\PimpleDumper\\Test\\": ["tests/"]
        }
    }
}
